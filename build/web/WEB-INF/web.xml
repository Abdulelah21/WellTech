<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AddFeedbackEntry</servlet-name>
        <servlet-class>AddFeedbackEntry</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddNewRepairRequest</servlet-name>
        <servlet-class>AddNewRepairRequest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BrowseAllRepairRecords</servlet-name>
        <servlet-class>BrowseAllRepairRecords</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletAddnewRequest</servlet-name>
        <servlet-class>servletAddnewRequest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletIndex</servlet-name>
        <servlet-class>servletIndex</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletFeedback</servlet-name>
        <servlet-class>servletFeedback</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddFeedbackEntry</servlet-name>
        <url-pattern>/AddFeedbackEntry</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddNewRepairRequest</servlet-name>
        <url-pattern>/AddNewRepairRequest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BrowseAllRepairRecords</servlet-name>
        <url-pattern>/BrowseAllRepairRecords</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletAddnewRequest</servlet-name>
        <url-pattern>/servletAddnewRequest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletIndex</servlet-name>
        <url-pattern>/servletIndex</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletFeedback</servlet-name>
        <url-pattern>/servletFeedback</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
